#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/esm/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules/glob/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules:/home/workspace/nodejs/coding-assignments/assignment/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/archiver-utils@5.0.2/node_modules/archiver-utils/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/archiver-utils@5.0.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/esm/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules/glob/dist/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules/glob/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/glob@10.3.10/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules:/home/workspace/nodejs/coding-assignments/assignment/node_modules:/home/workspace/nodejs/coding-assignments/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/archiver-utils@5.0.2/node_modules/archiver-utils/node_modules:/home/workspace/nodejs/coding-assignments/assignment/swivl_backend_assignment/node_modules/.pnpm/archiver-utils@5.0.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../glob@10.3.10/node_modules/glob/dist/esm/bin.mjs" "$@"
fi
